    "concurrently": "^5.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "wait-on": "^5.0.0"
